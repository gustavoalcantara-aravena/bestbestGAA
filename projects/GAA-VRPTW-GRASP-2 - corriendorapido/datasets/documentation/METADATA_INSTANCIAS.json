  "algorithm_parameters": {
    "distance_calculation": {
      "method": "euclidean",
      "formula": "sqrt((x2-x1)^2 + (y2-y1)^2)",
      "precision": "float"
    },
    "time_calculation": {
      "speed_assumption": "1 distance unit = 1 time unit",
      "waiting_time": "allowed (if arrival < ready_time)",
      "late_arrival": "forbidden (arrival must be <= due_date)"
    },
    "validation_constraints": {
      "capacity": {
        "type": "hard",
        "rule": "sum(demands_in_route) <= vehicle_capacity"
      },
      "time_windows": {
        "type": "hard",
        "rule": "arrival_time <= due_date for all customers"
      },
      "depot_return": {
        "type": "hard",
        "rule": "return_time <= depot.due_date"
      },
      "visit_once": {
        "type": "hard",
        "rule": "each customer visited exactly once"
      }
    }
  },
  
  "performance_benchmarks": {
    "computation_time_targets": {
      "C1": "1-5 seconds (heuristic), 10-60 seconds (metaheuristic)",
      "C2": "0.5-2 seconds (heuristic), 5-30 seconds (metaheuristic)",
      "R1": "5-30 seconds (heuristic), 30-300 seconds (metaheuristic)",
      "R2": "2-10 seconds (heuristic), 10-60 seconds (metaheuristic)",
      "RC1": "3-15 seconds (heuristic), 15-120 seconds (metaheuristic)",
      "RC2": "2-8 seconds (heuristic), 10-80 seconds (metaheuristic)"
    },
    "quality_metrics": {
      "gap_calculation": "((solution - bks) / bks) * 100",
      "acceptable_gap": {
        "excellent": "< 1%",
        "good": "1-5%",
        "acceptable": "5-10%",
        "poor": "> 10%"
      }
    }
  }
